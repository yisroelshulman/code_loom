## the valid input combinations with the check

# valid 1. input num, output num
check {
    input (
        num: 5
    )
    output (
        num: 10
    )
}

# valid 2. input num list, output num
check {
    input (
        num: list [ 5 10 ]
    )
    output (
        num: 20
    )
}

# valid 3. input num, output bool
check {
    input (
        num: 5
    )
    output (
        bool: true
    )
}

# valid 4. input num list, output bool
check {
    input (
        num: list [ 5 10]
    )
    output (
        bool: false
    )
}

# valid 5. input num, output str
check {
    input (
        num: 5
    )
    output (
        str: "valid"
    )
}

